--donut

local dt

d:AddTextBox('Enter Target',function(text)
    for _,v in pairs(game.Players:GetPlayers()) do
       if string.sub(v.Name,1,#text) then
           dt = v.Name
       end
    end
    
end)

d:AddButton('God',function()
    alert('Must have a stand that uses Donut.')
    local args = 
            {
                [1] = workspace:WaitForChild(lp.Name),
                [2] = -9e99
            }
            game:GetService("ReplicatedStorage"):WaitForChild("Main"):WaitForChild("Attacks"):WaitForChild("Donut"):FireServer(unpack(args))
    
end)

d:AddButton('Donut',function()
    alert('Must have a stand that uses Donut.')
    
    local args = 
            {
                [1] = workspace:WaitForChild(dt),
                [2] = 123
            }
            game:GetService("ReplicatedStorage"):WaitForChild("Main"):WaitForChild("Attacks"):WaitForChild("Donut"):FireServer(unpack(args))
            local args = 
            {
                [1] = workspace:WaitForChild(dt),
                [2] = 123
            }
            game:GetService("ReplicatedStorage"):WaitForChild("Main"):WaitForChild("Attacks"):WaitForChild("Donut"):FireServer(unpack(args))
    
end)

d:AddSwitch('Donut Spam',function(state)
    if state then
        alert('Must have a stand that uses Donut.')
        _G.on = true
        while _G.on do
            local args = 
            {
                [1] = workspace:WaitForChild(dt),
                [2] = 123
            }
            game:GetService("ReplicatedStorage"):WaitForChild("Main"):WaitForChild("Attacks"):WaitForChild("Donut"):FireServer(unpack(args))
            local args = 
            {
                [1] = workspace:WaitForChild(dt),
                [2] = 123
            }
            game:GetService("ReplicatedStorage"):WaitForChild("Main"):WaitForChild("Attacks"):WaitForChild("Donut"):FireServer(unpack(args))
            task.wait()
        end
    else
        _G.on = false
    end
    
end)


end


function ks()

local KSG = Instance.new("ScreenGui")
local KUI = Instance.new("Frame")
