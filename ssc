game:GetService("StarterGui"):SetCore("SendNotification",
{
Title = "CREATED BY MQKING#5855",
Text = "https://discord.gg/VbeKesjWtu"
}
)


getgenv().mHNWwk7jnS83X2DRBKTccpA9Q9Qfs6eTFfmszMrP7zsUULrHmxyMhaMVNmLPyH3kQsqUH5Pf5RYFEeFutg6KCAHVJ4ePspwT5vXSrT7tbQDBFvXHtMqw4JtbBY9tQp7 = "rizzoto"


