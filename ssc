function ba()


--GUI

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bardium/random/main/plsdonateoldui"))()

local Window = library:AddWindow("MQKING HUB",
	{
		main_color = Color3.fromRGB(0,0,0),
		min_size = Vector2.new(150, 235),
		toggle_key = Enum.KeyCode.RightShift,
	})

local f = Window:AddTab("F")
local u = Window:AddTab("U")
local t2 = Window:AddTab("2")
local bank = Window:AddTab("B")
local d = Window:AddTab('D')


local lp = game.Players.LocalPlayer


function alert(text)
   game:GetService("StarterGui"):SetCore("SendNotification",
{
Title = 'ALERT',
Text = text
}
) 
end

function binds()
    local w = library:AddWindow("Keybinds",
    	{
    		main_color = Color3.fromRGB(0,0,0),
    		min_size = Vector2.new(160, 205),
    		toggle_key = Enum.KeyCode.Comma,
    	})
    	
    --Tabs
    local keys = w:AddTab("Keys")
    --vars
    local UIS = game:GetService("UserInputService")
    local karskey
    local instakey
    local barragekey
    local toggle = false
    
    -- functions
    function kars()
       game:GetService("ReplicatedStorage").Moves.Kars.KarsHeavy:FireServer()

game:GetService("ReplicatedStorage").Moves.Kars.KarsHeavy:FireServer()


local args = {
    [1] = 99e99999
}

game:GetService("ReplicatedStorage").Moves.Kars.AirSlash:FireServer(unpack(args))

wait(.5)

local args = {
    [1] = 99e99999
}

game:GetService("ReplicatedStorage").Moves.Kars.AirSlash:FireServer(unpack(args))

wait(.5)

local args = {
    [1] = 99e99999
}

game:GetService("ReplicatedStorage").Moves.Kars.AirSlash:FireServer(unpack(args))

wait(.5)

local args = {
    [1] = 99e99999
}

game:GetService("ReplicatedStorage").Moves.Kars.AirSlash:FireServer(unpack(args))

wait(.3)

local args = {
    [1] = 99e99999
}

game:GetService("ReplicatedStorage").Moves.Kars.AirSlash:FireServer(unpack(args))

task.wait()

local args = {
    [1] = 99e99999
}

game:GetService("ReplicatedStorage").Moves.Kars.AirSlash:FireServer(unpack(args))

        
    end
    function barrage()
       game:GetService("ReplicatedStorage").Main.Attacks.Barrage:FireServer()
game:GetService("ReplicatedStorage").Main.Attacks.Barrage:FireServer()
game:GetService("ReplicatedStorage").Main.Attacks.Barrage:FireServer()
game:GetService("ReplicatedStorage").Main.Attacks.Barrage:FireServer()
game:GetService("ReplicatedStorage").Main.Attacks.Barrage:FireServer()

game:GetService("ReplicatedStorage").Main.Attacks.OHBarrage:FireServer()
game:GetService("ReplicatedStorage").Main.Attacks.OHBarrage:FireServer()
game:GetService("ReplicatedStorage").Main.Attacks.OHBarrage:FireServer()
game:GetService("ReplicatedStorage").Main.Attacks.OHBarrage:FireServer()
        
    end
    function insta()
    local args = {
    [1] = 99e99999
}

game:GetService("ReplicatedStorage").Main.Attacks.Gun:FireServer(unpack(args))

local args = {
    [1] = 99e99999
}

game:GetService("ReplicatedStorage").Main.Attacks.Gun:FireServer(unpack(args))

local args = {
    [1] = 99e99999
}

game:GetService("ReplicatedStorage").Main.Attacks.Gun:FireServer(unpack(args))

        
    end
    
    UIS.InputBegan:Connect(function(key)
        if (UIS:GetFocusedTextBox()) then return end
        if key.KeyCode == karskey then
            kars()
        end
    end)
    
    UIS.InputBegan:Connect(function(key)
        if key.KeyCode == instakey then
            insta()
        end
    end)
    
    UIS.InputBegan:Connect(function(key)
        if key.KeyCode == barragekey then
            toggle = not toggle
            while toggle do
                task.wait()
                barrage()
            end
        end
        
    end)
    
    
    local bindkars = keys:AddKeybind("Kars", function(key)
    	karskey = key
    end)
    
    local bindkill = keys:AddKeybind("Insta", function(key)
    	instakey = key
    end)
    
    local bindbarrage = keys:AddKeybind("Barrage", function(key)
    	barragekey = key
    end)

end

